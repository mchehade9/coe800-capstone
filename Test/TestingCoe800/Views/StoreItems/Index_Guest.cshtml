@model IEnumerable<TestingCoe800.Models.StoreItem>

@{
    ViewBag.Title = "Store Inventory";
}

<h2>Nike </h2>

<p>
  
</p>
<table class="table" style="background-color :white ;opacity: 0.9">

    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ItemName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Store.StoreName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ItemName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Store.StoreName)
            </td>
           
        </tr>
    }

</table>
